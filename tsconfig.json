{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true,
    "types": ["node"],
    "lib": ["es2017"]
  }
}
